<!doctype html>
<html lang="fr">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% include "metas.njk" %}
    <link rel="stylesheet" href="/assets/style.css">
</head>

<body>

    {% include "header.njk" %}

    <main>
        <h1>Devblog</h1>

        {%- for post in collections.devblog -%}
        <article>
            <a href="{{ post.url }}"><h2>{{ post.data.title }}</h2></a>
            <p>{{ post.data.description }}</p>
        </article>
        {%- endfor -%}
    </main>

    {% include "footer.njk" %}

</body>

</html>